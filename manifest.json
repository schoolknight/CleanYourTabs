{
  "name": "Clean Your Tabs",
  "description": "A extension to clean your Chrome tabs",
  "version": "0.1.1",
  "background": { "scripts": ["background.js"] },
  "options_page": "pages/options.html",
  "manifest_version": 2,
  "permissions":[
    "tabs",
    "storage",
    "notifications"
  ],
  "icons": {
        "16": "cyt_icon_128.png",
        "48": "cyt_icon_128.png",
        "128": "cyt_icon_128.png"
    },
  "commands": {
        "clean_tabs": {
            "suggested_key": {
                "windows": "Ctrl+B",
                "mac": "Command+B",
                "chromeos": "Ctrl+B",
                "linux": "Ctrl+B"
            },
            "description": "test"
        }
    }
}
